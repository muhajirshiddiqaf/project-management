{
  "name": "saas-project-management",
  "version": "1.0.0",
  "description": "SaaS Project Management & Quotation System",
  "main": "index.js",
  "scripts": {
    "start": "cd backend && npm start",
    "start:dev": "cd backend && npm run start:dev",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && yarn install",
    "dev": "concurrently \"cd backend && npm run start:dev\" \"cd frontend && yarn start\"",
    "dev:frontend": "cd frontend && yarn start",
    "dev:backend": "cd backend && npm run start:dev",
    "build": "cd frontend && yarn build",
    "test": "cd backend && npm test"
  },
  "keywords": [
    "saas",
    "project-management",
    "quotation",
    "nodejs",
    "react",
    "postgresql"
  ],
  "author": "Muhajir Shiddiq",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
